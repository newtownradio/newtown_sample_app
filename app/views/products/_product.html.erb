<div class="container" id="pro-place">
    <% @products.each do |product| %>
    <%= @product.image_url %>
    <%= product.name %>
    <%= product.description %>
    <%= product.colour %>
<% end %>
<br>   
    <%= link_to 'Show', product %>
    <%= link_to 'Edit', edit_product_path(product) %>
     <%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %>